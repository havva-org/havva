{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "updated",
    "series"
  ],
  "properties": {
    "updated": {
      "type": "string"
    },
    "series": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "code",
          "name",
          "unit",
          "wld"
        ],
        "properties": {
          "code": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "unit": {
            "type": "string"
          },
          "wld": {
            "type": "number"
          }
        }
      }
    }
  }
}